          }
        }
      },
      "Basket": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:44355",
            "MetaData": {
              "PublicAddress": "https://basket.eshoponabp.dev"
            }
          }
        }
      },
      "Ordering": {
